{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"compatibility_date": "2025-10-08",
	"name": "auto-wol",
	"build": {
		"command": "pnpm build"
	},
	"pages_build_output_dir": "./dist",
	"durable_objects": {
		"bindings": [
			{
				"class_name": "WolManager",
				"name": "WolManager"
			}
		]
	},
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true
}
